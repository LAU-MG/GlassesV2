    "gh-pages": "^5.0.0",
    "husky": "^9.1.1",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.7",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "stylelint": "^16.7.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.4.1",
    "typescript": "^5",
    "vitest": "^2.0.4"
  }
}
